{
  "name": "corytech/php-cs-fixer-rules",
  "type": "library",
  "description": "Custom cs-fixer rules",
  "keywords": ["php-cs-fixer"],
  "license": "MIT",
  "require": {
    "php": ">=8.4",
    "ext-filter": "*",
    "ext-tokenizer": "*",
    "friendsofphp/php-cs-fixer": "^3.84"
  },
  "require-dev": {
    "phpunit/phpunit": "^12"
  },
  "autoload": {
    "psr-4": {
      "Corytech\\PhpCsFixerRules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Corytech\\PhpCsFixerRules\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}